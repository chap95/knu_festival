<p id="notice"><%= notice %></p>

<h1>Losts</h1>

<table>
  <thead>
    <tr>
      <th>분 실 물</th>
      <th>제 목</th>
      <!--<th>Content</th>-->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @losts.each do |lost| %>
      <tr>
        <!--<td><!%= lost.content %></td>-->
            <td class="image-section">
              <img src="<%= lost.image %>">
            </td>
        <td><%= lost.title %></td>
        <td><%= link_to 'Show', lost %></td>
        <td><%= link_to 'Edit', edit_lost_path(lost) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lost', new_lost_path %>
